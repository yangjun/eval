<div class="row">
 <legend><h3 class="text-center">{{model.title}}</h3></legend>



    <div class="col-md-12">
     <div class="row">
      <div class="col-md-2">
        <p class="text-right">版本选择</p>
      </div>
      <div class="col-md-10">
        {{type-ahead url='data/repos.json' template='' selection=item   placeholder='选择版本'}}
      </div>
    </div> 

      {{radar-chart height=400 content=model}}
  <legend>   </legend>
		<p>详细信息</p> 
      <table class="table table-hover">

                <thead>
                <tr>
	              <th>序号</th>
			      <th>维度</th>
			      <th>分值</th>
                </tr>
                </thead>
                <tbody>
                  {{#each item in items}}
                  <form class="form-horizontal">
                  
                       <tr>
                          
                          <td>{{item.index}}</td>
					      <td>{{item.name}}</td>
					      <td>{{item.type}}</td>

                      </tr>
                      </from>
                  {{/each}}
                </tbody>
            </table>
         
    </div>
</div>