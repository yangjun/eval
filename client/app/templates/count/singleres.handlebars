

<div class="row">

<legend><h3 class="text-center">{{model.title}}</h3></legend>


    <div class="col-md-12">
        <div class="row">
        
      <div class="col-md-2">
        <p class="text-right">版本选择</p>
      </div>
      <div class="col-md-4">
        {{type-ahead url='data/repos.json' template='' selection=item   placeholder='选择不同的版本'}}
      </div>
      
      <div class="col-md-2">
        <p class="text-right">资源类别</p>
      </div>
      <div class="col-md-4">
	    <select class="form-control" placeholder='选择不同类别的资源'>  
		  <option>区域</option>
		  <option>站点</option>
		  <option>机房</option>
		  <option>机架</option>
		  <option>网元</option>
		  <option>光缆</option>
		  <option>传输段</option>
		</select>
     </div>
    </div>
<p>   </p>
    <!--雷达图-->
        {{vbar-chart height=400 content=model}}
        
       <p>   </p>
		<p>详细信息</p> 
      <table class="table table-hover">

                <thead>
                <tr>
	              <th>序号</th>
			      <th>维度</th>
			      <th>分值</th>
                </tr>
                </thead>
                <tbody>
                  {{#each item in items}}
                  <form class="form-horizontal">
                  
                       <tr>
                          
                          <td>{{item.index}}</td>
					      <td>{{item.name}}</td>
					      <td>{{item.type}}</td>

                      </tr>
                      </from>
                  {{/each}}
                </tbody>
            </table>
    </div>
</div>