<div class="row">
<legend><h3 class="text-center">{{model.title}}</h3></legend>
    <div class="col-md-12">
        
        <div class="row">
        
      <div class="col-md-2">
        <p class="text-right">开始时间</p>
      </div>
      <div class="col-md-4">
      <select class="form-control" placeholder='选择不同类别的资源'>  
		     <option>1月</option>
			 <option>2月</option>
			 <option>3月</option>
			 <option>4月</option>
			 <option>5月</option>
		     <option>6月</option>
			 <option>7月</option>
			 <option>8月</option>
			 <option>9月</option>
			 <option>10月</option>
		     <option>11月</option>
			 <option>12月</option>
		</select>
     </div>
      
      <div class="col-md-2">
        <p class="text-right">结束时间</p>
      </div>
      <div class="col-md-4">
	    <select class="form-control" placeholder='选择不同类别的资源'>  
		     <option>1月</option>
			 <option>2月</option>
			 <option>3月</option>
			 <option>4月</option>
			 <option>5月</option>
		     <option>6月</option>
			 <option>7月</option>
			 <option>8月</option>
			 <option>9月</option>
			 <option>10月</option>
		     <option>11月</option>
			 <option>12月</option>
		</select>
     </div>
    </div>
        
        
  
      {{line-chart height=400 content=model}}
      
      <legend>   </legend>
		<p>详细信息</p> 
      <table class="table table-hover">

                <thead>
                <tr>
	              <th>序号</th>
			      <th>时间点</th>
			      <th>分值</th>
                </tr>
                </thead>
                <tbody>
                  {{#each item in items}}
                  <form class="form-horizontal">
                  
                       <tr>
                          
                          <td>{{item.index}}</td>
					      <td>{{item.name}}</td>
					      <td>{{item.type}}</td>

                      </tr>
                      </from>
                  {{/each}}
                </tbody>
            </table>
    </div>
</div>