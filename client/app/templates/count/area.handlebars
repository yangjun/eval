

<div class="row">
<!--{{selectedEntity}}-->
<legend><h3 class="text-center">区域-总体质量</h3></legend>

    <div class="col-sm-12">
        <div class="row">

      <div class="col-md-4">
        <p class="text-right">版本选择</p>
      </div>
      <div class="col-md-8">
      {{type-ahead url='evaluate-web/rest/evaluate/evaluateVersion' template='' selection=version placeholder='选择不同的版本'}}
      </div>
   

    </div>
<p>   </p>
    <!--雷达图-->
        {{vbar-chart height=400 content = data}}
        
       <p>   </p>
		<p>详细信息</p> 
      <table class="table table-hover">

                <thead>
                <tr>
	              <th>序号</th>
			      <th>维度</th>
			      <th>分值</th>
                </tr>
                </thead>
                <tbody>
                  {{#each item in data.items}}
                  <form class="form-horizontal">
                  
                       <tr>
                          
                          <td>{{item.index}}</td>
					      <td>{{item.dimension}}</td>
					      <td>{{item.score}}</td>

                      </tr>
                      </from>
                  {{/each}}
                </tbody>
            </table>
    </div>
</div>